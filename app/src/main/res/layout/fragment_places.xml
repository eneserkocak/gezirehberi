<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">



    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/back_screen"
        tools:context=".viewMain.PlacesFragment"
        >


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/parksBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/parkl"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"></de.hdodenhof.circleimageview.CircleImageView>


        <TextView
            android:id="@+id/list"
            android:layout_width="126dp"
            android:layout_height="48dp"
            android:layout_marginTop="1dp"
            android:text="Parklar ve Mesire Alanları"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/parksBtn"
            app:layout_constraintStart_toStartOf="@+id/parksBtn"
            app:layout_constraintTop_toBottomOf="@+id/parksBtn"></TextView>

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/lakesBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"
            android:src="@drawable/bys"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView
            android:id="@+id/lis"
            android:layout_width="126dp"
            android:layout_height="48dp"
            android:layout_marginTop="1dp"
            android:text="Göller ve Mağaralar"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/lakesBtn"
            app:layout_constraintStart_toStartOf="@+id/lakesBtn"
            app:layout_constraintTop_toBottomOf="@+id/lakesBtn"></TextView>


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/historicalBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/historical"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/list"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView
            android:id="@+id/li"
            android:layout_width="126dp"
            android:layout_height="24dp"
            android:layout_marginTop="1dp"
            android:text="Tarihi Yapılar"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/historicalBtn"
            app:layout_constraintStart_toStartOf="@+id/historicalBtn"
            app:layout_constraintTop_toBottomOf="@+id/historicalBtn"></TextView>


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/museumsBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/ms"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lis"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView

            android:id="@+id/textView4"
            android:layout_width="126dp"
            android:layout_height="24dp"
            android:layout_marginTop="1dp"
            android:text="Müzeler"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/museumsBtn"
            app:layout_constraintStart_toStartOf="@+id/museumsBtn"
            app:layout_constraintTop_toBottomOf="@+id/museumsBtn"></TextView>


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/mosquesBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="32dp"
            android:src="@drawable/cami"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/li"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView

            android:id="@+id/textView6"
            android:layout_width="126dp"
            android:layout_height="48dp"
            android:layout_marginTop="1dp"
            android:text="Cami ve Medreseler"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/mosquesBtn"
            app:layout_constraintStart_toStartOf="@+id/mosquesBtn"
            app:layout_constraintTop_toBottomOf="@+id/mosquesBtn"></TextView>


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/ancientcityBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"
            android:src="@drawable/kilistra"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView

            android:layout_width="126dp"
            android:layout_height="48dp"
            android:layout_marginTop="1dp"
            android:text="Antik Kent ve Örenler"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/ancientcityBtn"
            app:layout_constraintStart_toStartOf="@+id/ancientcityBtn"
            app:layout_constraintTop_toBottomOf="@+id/ancientcityBtn"></TextView>

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/caravanseraiBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/caravanserai"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView

            android:id="@+id/textView7"
            android:layout_width="126dp"
            android:layout_height="24dp"
            android:layout_marginTop="1dp"
            android:text="Kervansaraylar"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/caravanseraiBtn"
            app:layout_constraintStart_toStartOf="@+id/caravanseraiBtn"
            app:layout_constraintTop_toBottomOf="@+id/caravanseraiBtn"></TextView>


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/churchesBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"
            android:src="@drawable/church"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView

            android:layout_width="126dp"
            android:layout_height="24dp"
            android:layout_marginTop="1dp"
            android:text="Kiliseler"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/churchesBtn"
            app:layout_constraintStart_toStartOf="@+id/churchesBtn"
            app:layout_constraintTop_toBottomOf="@+id/churchesBtn"></TextView>


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/bathBtn"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/bath"
            app:civ_border_color="@color/black"
            app:civ_border_width="3dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7"></de.hdodenhof.circleimageview.CircleImageView>

        <TextView

            android:layout_width="126dp"
            android:layout_height="48dp"
            android:layout_marginTop="1dp"
            android:text="Hamam ve Kaplıcalar"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/bathBtn"
            app:layout_constraintStart_toStartOf="@+id/bathBtn"
            app:layout_constraintTop_toBottomOf="@+id/bathBtn"></TextView>

    </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>
</layout>